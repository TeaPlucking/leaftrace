<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient_background"
    tools:context=".DashboardActivity">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="374dp"
        android:layout_height="649dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="19dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.567"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/white" />

    <TextView
        android:id="@+id/about"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/about_img"
        android:layout_alignEnd="@+id/imageView7"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="60dp"
        android:text="About"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/about_img" />

    <TextView
        android:id="@+id/setting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/setting_img"
        android:layout_alignStart="@+id/imageView7"
        android:layout_marginStart="55dp"
        android:layout_marginTop="-1dp"
        android:text="Settings"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/setting_img" />

    <TextView
        android:id="@+id/issue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/issue_img"
        android:layout_alignEnd="@+id/imageView7"
        android:layout_marginTop="-2dp"
        android:layout_marginEnd="65dp"
        android:text="Issues"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/issue_img" />

    <ImageView
        android:id="@+id/payment_img"
        android:layout_width="152dp"
        android:layout_height="96dp"
        android:layout_alignStart="@+id/imageView7"
        android:layout_alignTop="@+id/imageView7"
        android:layout_marginStart="18dp"
        android:layout_marginTop="123dp"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/name_txt"
        app:srcCompat="@drawable/payment" />

    <TextView
        android:id="@+id/payment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/payment_img"
        android:layout_alignStart="@+id/imageView7"
        android:layout_marginStart="59dp"
        android:layout_marginTop="-2dp"
        android:text="Payment"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/payment_img" />

    <TextView
        android:id="@+id/area"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/areaimg"
        android:layout_alignEnd="@+id/imageView7"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="61dp"
        android:text="Area"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/areaimg" />

    <TextView
        android:id="@+id/profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/profile_img"
        android:layout_alignStart="@+id/imageView7"
        android:layout_marginStart="64dp"
        android:layout_marginTop="0dp"
        android:text="Profile"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/profile_img" />

    <ImageView
        android:id="@+id/profile_img"
        android:layout_width="121dp"
        android:layout_height="112dp"
        android:layout_below="@+id/payment"
        android:layout_alignStart="@+id/imageView7"
        android:layout_marginStart="27dp"
        android:layout_marginTop="42dp"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/payment"
        app:srcCompat="@drawable/profile" />

    <ImageView
        android:id="@+id/about_img"
        android:layout_width="155dp"
        android:layout_height="135dp"
        android:layout_below="@+id/area"
        android:layout_alignEnd="@+id/imageView7"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/area"
        app:srcCompat="@drawable/aboutus" />

    <ImageView
        android:id="@+id/setting_img"
        android:layout_width="128dp"
        android:layout_height="111dp"
        android:layout_below="@+id/profile"
        android:layout_alignStart="@+id/imageView7"
        android:layout_marginStart="30dp"
        android:layout_marginTop="31dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/profile"
        app:layout_constraintVertical_bias="0.666"
        app:srcCompat="@drawable/settings" />

    <ImageView
        android:id="@+id/issue_img"
        android:layout_width="138dp"
        android:layout_height="116dp"
        android:layout_below="@+id/about"
        android:layout_alignEnd="@+id/imageView7"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/about"
        app:srcCompat="@drawable/issues" />

    <ImageView
        android:id="@+id/areaimg"
        android:layout_width="126dp"
        android:layout_height="90dp"
        android:layout_alignTop="@+id/imageView7"
        android:layout_alignEnd="@+id/imageView7"
        android:layout_marginTop="127dp"
        android:layout_marginEnd="21dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/name_txt"
        app:srcCompat="@drawable/area" />

    <TextView
        android:id="@+id/hello_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView7"
        android:layout_alignTop="@+id/imageView7"
        android:layout_marginStart="18dp"
        android:layout_marginTop="26dp"
        android:text="Hello,"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

    <TextView
        android:id="@+id/name_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/hello_txt"
        android:layout_alignStart="@+id/imageView7"
        android:layout_marginStart="12dp"
        android:layout_marginTop="3dp"
        android:text="Data Pirates"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/hello_txt" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="400dp"
        android:layout_height="48dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="6dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/green" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="43dp"
        android:layout_height="40dp"
        android:layout_below="@+id/imageView7"
        android:layout_alignStart="@+id/imageView14"
        android:layout_marginStart="28dp"
        android:layout_marginTop="7dp"
        app:srcCompat="@drawable/home" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="40dp"
        android:layout_height="38dp"
        android:layout_below="@+id/imageView7"
        android:layout_alignParentStart="true"
        android:layout_marginStart="131dp"
        android:layout_marginTop="8dp"
        app:srcCompat="@drawable/message" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="55dp"
        android:layout_height="50dp"
        android:layout_below="@+id/imageView7"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="140dp"
        app:srcCompat="@drawable/user" />

</RelativeLayout>